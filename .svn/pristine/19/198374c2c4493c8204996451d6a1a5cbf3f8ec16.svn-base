.mine-vip {
  background-color: #f5f5f5;
  height: 100vh;
}

.vip-head {
  height: 136rpx;
  background-color: #fff;
  text-align: center;
  padding: 40rpx;
  position: relative;
}

.mine-info {
  margin: 0 auto;
  position: relative;
  width: 400rpx;
}

.mine-info image {
  position: absolute;
  left: 50%;
  transform: translateX(-105%);
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
  border: 4rpx solid #7d7d7d;
  margin-right: 30rpx;
}

.mine-txt {
  position: absolute;
  left: 55%;
  font-size: 36rpx;
}

.mine-name {
  width: 320rpx;
  color: #666;
  font-size: 32rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  text-align: left;
}

.mine-level {
  color: #fff;
  font-size: 24rpx;
  line-height: 35rpx;
  border-radius: 10rpx;
  background-color: #626262;
  width: 80rpx;
  text-align: center;
  margin: 15rpx 0;
}

.vip-endtime {
  color: #9b9b9b;
  font-size: 22rpx;
  text-align: left;
}

.vip-level {
  margin: 30rpx 0;
  padding: 50rpx;
  background-color: #fff;
}

.vip-list {
  display: flex;
}

.vip-list image {
  flex: 1;
  float: left;
  width: 200rpx;
  height: 200rpx;
}

.img-level {
  flex: 1.5;
  width: 250rpx !important;
  margin-top: -25rpx;
}

.vip-foot {
  padding: 30rpx;
}

.vip-buy {
  background-color: #fff;
  text-align: center;
  line-height: 80rpx;
  font-size: 32rpx;
  padding: 30rpx;
}

.vip-time {
  color: #666;
}

.vip-price {
  color: #e64731;
  font-size: 40rpx;
}

.vip-cost {
  color: #9a9a9a;
  text-decoration: line-through;
}

.vip-coupon {
  /* color: #666;
  width: 400rpx; */
  position: relative;
  left: 50%;
  transform: translateX(-50%);
}

.my-coupon {
  color: #9a9a9a;
  margin-left: 30rpx;
  width: 250rpx;
}

.vip-coupon icon {
  color: #666;
  font-size: 30rpx;
}

.vip-buy-btn {
  color: #fff;
  background-color: #e64731;
  font-size: 30rpx;
  width: 200rpx; 
  line-height: 60rpx;
  border-radius: 10rpx;
  margin: 0 auto;
  text-align: center;
  padding: 0 20rpx;
}

/* .vip-buy-btn:hover {
} */
.model-bg {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0,0,0,0.5);
  transition: all 1s;
}
.coupon-box {
  width: 690rpx;
  height: 600rpx;
  overflow: scroll;
  background-color: #fff;
  position: fixed;
  transform: translateY(0);
  transition: all 1s;
}

.coupon-list {
    /* padding: 30rpx 30rpx 50rpx 30rpx; */
    padding: 30rpx 30rpx 50rpx 30rpx;
}

.coupon-list .coupon-item {
    height: 152rpx;
    width: 682rpx;
    position: relative;
}

.coupon-list .coupon-item.coupon-status-1 .coupon-right,.coupon-list .coupon-item.coupon-status-2 .coupon-right {
    color: rgba(0,0,0,.35)!important;
}

.coupon-list .coupon-item .coupon-bg {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: -1;
}

.coupon-list .coupon-item .coupon-status-icon {
    width: 140rpx;
    height: 98rpx;
    position: absolute;
    top: 0;
    right: 8rpx;
    z-index: 1;
}

.coupon-list .coupon-item .coupon-left {
    color: #fff;
    width: 202rpx;
}

.coupon-list .coupon-item .coupon-right {
    padding: 20rpx 10rpx;
}

.user_coupon_font {
    font-size: 8pt;
    color: #666666;
    margin-top: 10rpx;
}

.user_coupon {
    font-size: 8pt;
    color: #666666;
}