.mine-balance {
}

.balance-head {
  padding: 0 30rpx;
  background-color: #e64731;
  color: #fff;
  line-height: 100rpx;
  height: 20vh;
}

.balance-count {
  font-size: 68rpx;
}

.income-list {
  height: 80vh;
}
.flex-row {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  flex-direction: row;
}

.flex-grow-0 {
  min-width: 0;
  -webkit-box-flex: 0;
  -ms-flex-positive: 0;
  flex-grow: 0;
  -ms-flex-negative: 0;
  flex-shrink: 0;
}

.flex-grow-1 {
  min-width: 0;
  -webkit-box-flex: 1;
  -ms-flex-positive: 1;
  flex-grow: 1;
  -ms-flex-negative: 1;
  flex-shrink: 1;
}


.income-road {
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 28rpx;
}

.income-one {
  padding: 30rpx;
  border-bottom: 1rpx solid #f6f6f6;
}
